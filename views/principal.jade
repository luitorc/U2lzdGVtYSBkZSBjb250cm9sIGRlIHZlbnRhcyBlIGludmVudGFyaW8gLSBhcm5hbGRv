					block __shoppingCart
		
	script.
		var typeDivisa = '$ ';
		$(window).ready(function(){
			$("#lpr#{idp}").css({'background-color':'#83FF67'});

			$.post('/config@getTypeDivisa', { //Get ProductsListInTb
					
			}, function(divisa, textStatus, xhr){
				typeDivisa = divisa;
